                "pretty_name",
                "Value #INFO",
                "nodename",
                "release",
                "Value #OS",
                "Value #UPTIME",
                "Value #LOAD1",
                "Value #LOAD5",
                "Value #LOAD15",
                "Value #CPUCOUNT",
                "Value #CPUUSAGE",
                "Value #MEMTOTAL",
                "Value #MEMUSAGE",
                "Value #FSTOTAL",
                "Value #FSUSAGE",
                "version_codename"
              ],
              "pattern": "instance|nodename|Value.+"
