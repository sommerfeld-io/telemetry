          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Value #INFO": true,
              "Value #LOAD15": true,
              "Value #LOAD5": true,
              "Value #OS": true,
              "instance": true
            },
            "includeByName": {},
            "indexByName": {
              "Value #CPUCOUNT": 8,
              "Value #CPUUSAGE": 9,
              "Value #FSTOTAL": 12,
              "Value #FSUSAGE": 13,
              "Value #INFO": 1,
              "Value #LOAD1": 5,
              "Value #LOAD15": 7,
              "Value #LOAD5": 6,
              "Value #MEMTOTAL": 10,
              "Value #MEMUSAGE": 11,
              "Value #OS": 3,
              "Value #UPTIME": 4,
              "instance": 0,
              "nodename": 2,
              "pretty_name": 14,
              "release": 16,
              "version_codename": 15
            },
            "renameByName": {
              "Value #CPUCOUNT": "Cores",
              "Value #CPUUSAGE": "CPU usage",
              "Value #CRITICAL": "Crit Alerts",
              "Value #FSTOTAL": "Root disk size",
              "Value #FSUSAGE": "Root disk usage",
              "Value #LOAD1": "Load 1m",
              "Value #LOAD15": "Load 15m",
              "Value #LOAD5": "Load 5m",
              "Value #MEMTOTAL": "Memory total",
              "Value #MEMUSAGE": "Memory usage",
              "Value #UPTIME": "Uptime",
              "Value #WARNING": "Warnings",
              "instance": "Instance",
              "nodename": "Hostname",
              "pretty_name": "OS",
              "release": "Kernel version",
              "version_codename": "Codename"
            }
          }
        }
      ],
      "type": "table"
    },
